<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.content_manager_service.dao.TopicReplyDao" >

    <insert id="createTopicReply" parameterType="com.model.TopicReplyVO">
        INSERT INTO topic_reply
        (topic_id,user_id,comment)
        VALUES
        (#{topicId},#{userId},#{comment})
    </insert>




</mapper>
<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.content_manager_service.dao.WishReplyDao" >

    <insert id="saveWishReply" parameterType="com.model.WishReplyVO">
        INSERT INTO wish_reply
        (wish_id,user_id,message)
        VALUES
        (#{wishId},#{userId},#{message})
    </insert>




</mapper>
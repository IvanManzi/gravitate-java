<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.content_manager_service.dao.PerformanceFeedbackDao" >

    <insert id="addPerformanceFeedback" parameterType="com.model.PerformanceFeedbackVO">
        INSERT INTO performance_feedback
        (done_by,user_id,year,quarter,sprint,feedback)
        VALUES
        (#{doneBy},#{userId},#{year},#{quarter},#{sprint},#{feedback})
    </insert>




</mapper>